{
	"compilerOptions": {
		"moduleResolution": "Node",
		"noImplicitAny": true,
		"removeComments": true,
		"declaration": true,
		"target": "ES5",
		"module": "CommonJS"
	},
	"typeRoots": [
		"node_modules/@types"
	],
	"types": [
		"jquery"
	],
	"awesomeTypescriptLoaderOptions": {
		"useCache": true
	},
	"exclude": [
		"node_modules",
		"test"
	]
}
